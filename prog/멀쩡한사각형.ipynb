    "import math\n",
    "\n",
    "def solution(w,h):\n",
    "    \n",
    "    if w == 1 or h == 1:\n",
    "        answer=0\n",
    "    else :\n",
    "        answer=w*h-(w+h-math.gcd(w,h))\n",
    "        \n",
    "    return answer"
   ]
